{
	"$schema": "https://turbo.build/schema.json",
	"globalEnv": [
		"GITHUB_TOKEN",
		"GITHUB_USERNAME",
		"GITHUB_REPO",
		"WAKATIME_API_KEY",
		"WAKATIME_DATA_URL",
		"RESEND_API_KEY",
		"UMAMI_USER_ID",
		"UMAMI_WEBSITE_ID",
		"UMAMI_API_KEY",
		"UMAMI_API_CLIENT_ENDPOINT",
		"UMAMI_SCRIPT",
		"UMAMI_PREVIEW_ENDPOINT",
		"TURBO_TOKEN",
		"TURBO_TEAM",
		"UPSTASH_REDIS_REST_URL",
		"UPSTASH_REDIS_REST_TOKEN",
		"AUTH_SECRET",
		"NEXT_PUBLIC_WEBSITE_URL",
		"NEXT_PUBLIC_NAME",
		"NEXT_PUBLIC_SURNAME",
		"NEXT_PUBLIC_WEBSITE_PREFIX",
		"NEXT_PUBLIC_GITHUB_USERNAME",
		"NEXT_PUBLIC_EMAIL",
		"NEXT_PUBLIC_PHONE",
		"NEXT_PUBLIC_UMAMI_PREVIEW_ENDPOINT"
	],
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": [".next/**", "!.next/cache/**"],
			"env": [
				"GITHUB_TOKEN",
				"GITHUB_USERNAME",
				"GITHUB_REPO",
				"WAKATIME_API_KEY",
				"WAKATIME_DATA_URL",
				"WAKATIME_DATA_SHARE_URL",
				"RESEND_API_KEY",
				"UMAMI_USER_ID",
				"UMAMI_WEBSITE_ID",
				"UMAMI_API_KEY",
				"UMAMI_API_CLIENT_ENDPOINT",
				"UMAMI_SCRIPT",
				"UMAMI_PREVIEW_ENDPOINT",
				"TURBO_TOKEN",
				"TURBO_TEAM",
				"UPSTASH_REDIS_REST_URL",
				"UPSTASH_REDIS_REST_TOKEN",
				"AUTH_SECRET",
				"AUTH_GITHUB_ID",
				"AUTH_GITHUB_SECRET",
				"NEXT_PUBLIC_WEBSITE_URL",
				"NEXT_PUBLIC_NAME",
				"NEXT_PUBLIC_SURNAME",
				"NEXT_PUBLIC_WEBSITE_PREFIX",
				"NEXT_PUBLIC_GITHUB_USERNAME",
				"NEXT_PUBLIC_EMAIL",
				"NEXT_PUBLIC_PHONE",
				"NEXT_PUBLIC_BIRTH_YEAR",
				"NEXT_PUBLIC_START_DEVELOPING_YEAR",
				"NEXT_PUBLIC_UMAMI_PREVIEW_ENDPOINT"
			]
		},
		"check-types": {
			"dependsOn": ["^check-types"]
		},
		"dev": {
			"persistent": true,
			"cache": false
		}
	}
}
