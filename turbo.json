{
	"$schema": "https://turbo.build/schema.json",
	"globalEnv": [
		"GITHUB_TOKEN",
		"GITHUB_USERNAME",
		"GITHUB_REPO",
		"TURBO_TOKEN",
		"TURBO_TEAM",
		"UPSTASH_REDIS_REST_URL",
		"UPSTASH_REDIS_REST_TOKEN"
	],
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": [".next/**", "!.next/cache/**"],
			"env": [
				"GITHUB_TOKEN",
				"GITHUB_USERNAME",
				"GITHUB_REPO",
				"TURBO_TOKEN",
				"TURBO_TEAM",
				"UPSTASH_REDIS_REST_URL",
				"UPSTASH_REDIS_REST_TOKEN"
			]
		},
		"check-types": {
			"dependsOn": ["^check-types"]
		},
		"dev": {
			"persistent": true,
			"cache": false
		}
	}
}
